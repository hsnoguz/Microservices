
@{
    ViewData["Title"] = "Index";
}
@model UserViewModel

<div class="row">
    <div action="col-md-9">
            <div class="card-body">
                <h5 class="card-title">Kullanıcı Bilgileri</h5>
                <table class="table table-striped">
                    <tr>
                        <td>Kullanıcı Adı</td>
                    </tr>
                    <tr>
                        <td>@Model.UserName</td>
                    </tr>
                    <tr>
                        <td>@Model.Email</td>
                    </tr>
                    <tr>
                        <td>@Model.City</td>

                    </tr>
                </table>
            </div>
    </div>
</div>

